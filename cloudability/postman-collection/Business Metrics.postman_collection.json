	"item": [
		{
			"name": "SavingsPlan Coverage Rates Metric",
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "username",
							"value": "{{API_KEY}}",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Accept",
						"value": "{{ACCEPT}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"Savings Plan Coverage percentage\",\n    \"numberFormat\": \"number\",\n    \"defaultValueExpression\": \"0\",\n    \"statements\": [\n        {\n            \"matchExpression\": \"DIMENSION['lease_type'] == 'On-Demand' || DIMENSION['lease_type'] == 'Reserved' || DIMENSION['lease_type'] == 'Savings Plan'\",\n            \"valueExpression\": \"(METRIC['on_demand_hours'] + METRIC['reserved_hours']) / METRIC['usage_hours']\"\n        }\n    ]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://api.cloudability.com/v3/internal/business-mappings/metrics",
					"protocol": "https",
					"host": [
						"api",
						"cloudability",
						"com"
					],
					"path": [
						"v3",
						"internal",
						"business-mappings",
						"metrics"
					]
				},
				"description": "Used to evaluate savings plan coverage rates based on exclusively on_demand_hours / usage_hours"
			},
			"response": []
		},
		{
			"name": "Currency Converter Metric",
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
